curl -X POST "http://localhost:8080/users" -d "availableBalance=100.00" -d "reservedBalance=50.00"

curl -X POST "http://localhost:8080/users" -d "availableBalance=100.00"

curl -X GET "http://localhost:8080/users/1"

curl -X GET "http://localhost:8080/users/2"

curl -X POST "http://localhost:8080/transactions/deposit/1" -H "Content-Type: application/json" -d "100.50"

curl -X POST "http://localhost:8080/transactions/deposit/2" -H "Content-Type: application/json" -d "330.30"

curl -X POST "http://localhost:8080/transactions/withdrawal/1" -H "Content-Type: application/json" -d "77.50"

curl -X POST "http://localhost:8080/transactions/withdrawal/2" -H "Content-Type: application/json" -d "50.00"

curl -X POST "http://localhost:8080/transactions/transfer/1/2" -H "Content-Type: application/json" -d "25.00"

curl -X POST "http://localhost:8080/reserves" \
     -H "Content-Type: application/x-www-form-urlencoded" \
     -d "userId=1&reserveAmount=50.00&serviceId=1&orderId=1"

curl -X POST "http://localhost:8080/reserves/1/cancel"

curl -X GET "http://localhost:8080/users/1"

curl -X POST "http://localhost:8080/reserves" \
     -H "Content-Type: application/x-www-form-urlencoded" \
     -d "userId=2&reserveAmount=101.30&serviceId=1&orderId=1"

curl -X GET "http://localhost:8080/reserves/2"

curl -X POST "http://localhost:8080/reserves/2/complete"

curl -X GET "http://localhost:8080/users/2"










